        <messages/>
        <name>Change prices based on selected option</name>
        <order/>
        <published_ref/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }

    // Map the prices for different pallet sizes
    var priceMap = {
        'small': 100,  // Price for small pallet
        'med': 200,    // Price for medium pallet
        'big': 300     // Price for big pallet
    };

    // Log the selected value
    jslog("Selected pallet size: " + newValue);

    // Get the price for the selected size
    var selectedPrice = priceMap[newValue];

    if (selectedPrice) {
        // Display the price in the calculated_price variable
        g_form.setValue('calculated_price', selectedPrice + ' USD');
    } else {
        g_form.setValue('calculated_price', 'N/A');
    }
}
]]></script>
