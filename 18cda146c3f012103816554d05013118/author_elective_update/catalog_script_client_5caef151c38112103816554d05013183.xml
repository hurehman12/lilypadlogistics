        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue === '') {
      return;
   }

   // Get the selected size value
   var selectedSize = g_form.getValue('select_a_size');

   // Define weight capacities based on the size
   var weightCapacity;
   switch (selectedSize) {
      case 'Small':
         weightCapacity = '100 kg';  // Set the weight capacity for Small size
         break;
      case 'Medium':
         weightCapacity = '500 kg';  // Set the weight capacity for Medium size
         break;
      case 'Large':
         weightCapacity = '1000 kg'; // Set the weight capacity for Large size
         break;
      default:
         weightCapacity = '';
   }

   // Set the value for the weight_capacity variable
   g_form.setValue('weight_capacity', weightCapacity);
}
]]></script>
